<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE wuic SYSTEM 'http://capgemini.com/wuic/wuic.dtd'>
<wuic>
	<resource-factory-builders>
        <resource-factory-builder id="sshFactoryBuilder" class="SshNutDaoBuilder">
            <properties>
                <property key="c.g.wuic.regex">true</property>
                <property key="c.g.wuic.login">wuic</property>
                <property key="c.g.wuic.password">wuic</property>
                <property key="c.g.wuic.basePath">java.io.tmpdir</property>
                <property key="c.g.wuic.basePathAsSystemProperty">true</property>
                <property key="c.g.wuic.serverPort">9876</property>
            </properties>
        </resource-factory-builder>
	</resource-factory-builders>
	<groups>
		<group id="css-image" configuration="yuicompressor-css" default-builder="sshFactoryBuilder">
            <file>junit*.css</file>
		</group>
	</groups>
	<configurations>
        <configuration id="yuicompressor-css" type="CSS">
            <aggregate>true</aggregate>
            <charset>UTF-8</charset>
            <compress>true</compress>
            <yuiLineBreakPos>-1</yuiLineBreakPos>
        </configuration>
	</configurations>
</wuic>