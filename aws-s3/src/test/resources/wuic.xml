<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE wuic SYSTEM 'http://capgemini.com/wuic/wuic.dtd'>
<wuic>
	<resource-factory-builders>
        <resource-factory-builder id="s3FactoryBuilder" class="S3NutDaoBuilder">
            <properties>
                <!--    Key : c.g.wuic.dao.cloudBucket
                        The bucket name, you can create a new bucket in your console :
                        https://console.aws.amazon.com/nut/ -->
                <property key="c.g.wuic.dao.cloudBucket">bucket-wuic-test</property>
                <!--    Key : c.g.wuic.dao.login
                        The AWS auth access key, you can generate a new auth access key in your account :
                        https://portal.aws.amazon.com/gp/aws/securityCredentials -->
                <property key="c.g.wuic.dao.login"></property>
                <!--    Key : c.g.wuic.dao.password
                        The AWS secret auth access key, you can generate a new secret auth access key in your account :
                        https://portal.aws.amazon.com/gp/aws/securityCredentials -->
                <property key="c.g.wuic.dao.password"></property>
                <!--    Key : c.g.wuic.dao.basePath
                        The nut path for search resources without bucket name, example :
                        empty : search in root and sub directories
                        wuic : search in "wuic/*"
                        wuic/test : search in "wuic/test/*" -->
                <property key="c.g.wuic.dao.basePath">wuic/test</property>
            </properties>
        </resource-factory-builder>
	</resource-factory-builders>
	<!--groups>
		<group id="css-image" configuration="yuicompressor-css" default-builder="s3FactoryBuilder">
            <path>.*.css</path>
		</group>
	</groups-->
	<configurations>
        <configuration id="yuicompressor-css" type="CSS">
            <aggregate>true</aggregate>
            <charset>UTF-8</charset>
            <compress>true</compress>
            <yuiLineBreakPos>-1</yuiLineBreakPos>
        </configuration>
	</configurations>
</wuic>