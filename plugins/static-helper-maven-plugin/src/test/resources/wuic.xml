<?xml version="1.0" encoding="UTF-8"?>

<wuic>
    <nut-dao-builders>
        <nut-dao-builder id="testClasspathNutDaoBuilder" type="ClasspathNutDaoBuilder">
            <properties>
                <property key="c.g.wuic.dao.regex">true</property>
                <property key="c.g.wuic.dao.proxyUris">http://localhost:9090/|http://localhost:8080/</property>
            </properties>
        </nut-dao-builder>
    </nut-dao-builders>

    <heaps>
        <heap id="js" dao-builder-id="testClasspathNutDaoBuilder">
            <nut-path>.*.js</nut-path>
        </heap>
        <heap id="css" dao-builder-id="testClasspathNutDaoBuilder">
            <nut-path>.*.css</nut-path>
        </heap>
        <heap id="html" dao-builder-id="testClasspathNutDaoBuilder">
            <nut-path>index.html</nut-path>
        </heap>
    </heaps>
</wuic>